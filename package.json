{
  "name": "oblique",
  "version": "0.1.0",
  "description": "An online bot that answers only obliquely",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:cli": "tsc --module NodeNext --moduleResolution NodeNext src/cli.ts --outDir dist-cli",
    "dev": "vite",
    "dev:cli": "tsx src/cli.ts",
    "start": "vite preview",
    "start:cli": "node dist-cli/cli.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "clean": "rimraf dist dist-cli"
  },
  "keywords": ["bot", "bluesky", "llm", "agent"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@atproto/api": "^0.12.0",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.0"
  }
}

